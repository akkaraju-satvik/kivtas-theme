{
    "name": "kivtas-theme",
    "displayName": "kivtaS Theme",
    "description": "A Vibrant Bluish Dark Theme",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "kivtaS Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/kivtaS Theme-color-theme.json"
            }
        ]
    },
    "icon": "images/Logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/akkaraju-satvik/kivtas-theme.git"
    },
    "scripts": {
        "compile:sass" : "node-sass ./sass/main.scss ./css/style.css -w"
    },
    "devDependencies": {
        "node-sass": "^5.0.0"
    }
}
